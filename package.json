{
  "name": "@emanoelims/eslint-config",
  "version": "0.2.0",
  "description": "ESlint configuration used by emanoelims",
  "private": false,
  "main": "index.js",
  "repository": {
    "url": "git+https://github.com/emanoelims/eslint-config.git"
  },
  "scripts": {},
  "access": "public",
  "keywords": [],
  "author": "Emanoel Mendes",
  "license": "MIT",
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "dependencies": {
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7"
  }
}
